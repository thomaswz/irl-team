rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Allow all authenticated users to read and write client documents for development
    match /client-documents/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
    
    // Default rule - deny all other access
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
